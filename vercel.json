{
  "buildCommand": "ROLLUP_SKIP_NODEJS_NATIVE=true npm run build",
  "installCommand": "npm install --no-package-lock",
  "framework": null,
  "rewrites": [
    { "source": "/((?!api|.*\\..*).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|avif)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)\\.(woff|woff2|ttf|otf)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ],
  "redirects": [
    { "source": "https://vertex-ai-rho.vercel.app/(.*)", "destination": "https://www.vertexed.app/$1", "permanent": true },
    { "source": "https://vertexed.app/(.*)", "destination": "https://www.vertexed.app/$1", "permanent": true }
  ],
  "build": {
    "env": {
      "ROLLUP_SKIP_NODEJS_NATIVE": "true",
      "NODE_VERSION": "20"
    }
  },
  "env": {
    "ROLLUP_SKIP_NODEJS_NATIVE": "true"
  }
}
